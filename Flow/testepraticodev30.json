  case \"🎯 Saber mais\":\n                    case \"🎯 Simple\":\n\n                        content['items'] = [\"Mantemos o foco no essencial. Menos é mais.\"];\n                        break;\n\n                    case '🚀 Saber mais':\n                    case \"🚀 Higher\":\n\n                        content['items'] = [\"Não temos medo de sonhar grande, pois sabemos que temos talento e determinação para realizar coisas incríveis!\"];\n                        break;\n                        \n                    default:\n                }\n\n                return JSON.stringify(content); //Return value will be saved as \"Return value variable\" field name\n            }","inputVariables":["userOption","content"],"outputVariable":"valueDetail","LocalTimeZoneEnabled":false},"conditions":[]}],"$leavingCustomActions":[],"$inputSuggestions":[],"$defaultOutput":{"stateId":"fallback","$invalid":false},"$tags":[],"id":"99c09338-11c7-4046-991d-1540e920471f","root":false,"$title":"Sobre o Valor","$position":{"top":"520px","left":"459px"},"$invalidContentActions":false,"$invalidOutputs":false,"$invalidCustomActions":false,"$invalid":false},"welcome":{"$contentActions":[{"action":{"$id":"a6999a50-82b9-4a5a-9603-2e1ea1f34d6e","$typeOfContent":"","type":"SendMessage","settings":{"id":"00000000-0000-0000-0000-000000000000","type":"application/vnd.lime.chatstate+json","content":{"state":"composing","interval":"1500"},"metadata":{}},"$cardContent":{"document":{"id":"00000000-0000-0000-0000-000000000000","type":"application/vnd.lime.chatstate+json","content":{"state":"composing","interval":"1500"}},"editable":true,"deletable":true,"position":"left","editing":false}},"$invalid":false},{"action":{"$id":"67ce1b32-89c6-40bd-8669-11589deced7f","$typeOfContent":"","type":"SendMessage","settings":{"id":"00000000-0000-0000-0000-000000000001","type":"text/plain","content":"Olá, espero que esteja tudo bem.\nSou a Lora responsável por passar os valores da Take.","metadata":{}},"$cardConten